<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>contesterList</servlet-name>
        <servlet-class>contesterList</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>saveName</servlet-name>
        <servlet-class>saveName</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>winnerList</servlet-name>
        <servlet-class>winnerList</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>contesterList</servlet-name>
        <url-pattern>/contesterList</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>saveName</servlet-name>
        <url-pattern>/saveName</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>winnerList</servlet-name>
        <url-pattern>/winnerList</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
